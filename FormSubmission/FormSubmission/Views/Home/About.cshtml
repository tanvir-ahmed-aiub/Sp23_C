@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>
<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Id</th>
        <th>Price</th>
    </tr>
    @for (int i = 0; i < Model.Length; i++) { 
        <tr>
            <td>@Model[i].Name</td>
            <td>@Model[i].Id</td>
            <td>@Model[i].Price</td>
        </tr>
    }
</table>